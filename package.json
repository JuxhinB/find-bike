{
  "name": "bike-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client:start": "react-scripts start",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test",
    "client:eject": "react-scripts eject",
    "server:tsc": "(cd server && tsc)",
    "server:tsc-watch": "(cd server && tsc -w)",
    "server:deploy": "(cd server && node index.js)",
    "server:watch-deploy": "(cd server && nodemon index.js)",
    "server:dev": "(cd server && concurrently \"npm run server:tsc-watch\" \"npm run server:watch-deploy\")",
    "build:prod": "npm run client:build && npm run server:tsc && npm run server:deploy"
  },
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
