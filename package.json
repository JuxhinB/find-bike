{
  "name": "bike-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "client:build-style": "postcss src/theme/style/style-raw.scss -o src/theme/style/style.scss",
    "client:start": "npm run client:build-style && react-app-rewired start",
    "client:build": "npm run client:build-style && react-app-rewired build",
    "client:test": "react-app-rewired test",
    "client:eject": "react-app-rewired eject",
    "client:format": "npx prettier --write 'src/**/*'",
    "server:tsc": "(cd server && tsc)",
    "server:tsc-watch": "(cd server && tsc -w)",
    "server:deploy": "(cd server && node index.js)",
    "server:watch-deploy": "(cd server && nodemon index.js)",
    "server:dev": "export NODE_ENV=development && (cd server && concurrently \"npm run server:tsc-watch\" \"npm run server:watch-deploy\")",
    "server:format": "npx prettier --write 'server/**/*'",
    "build:prod": "npm run client:build && npm run server:tsc && npm run server:deploy"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.0",
    "antd": "^4.12.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "formik": "^2.2.6",
    "leaflet": "^1.7.1",
    "mongoose": "^5.11.15",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.2.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0",
    "react-localization": "^1.0.15",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-transition-group": "^4.4.1",
    "remove": "^0.1.5",
    "sass-loader": "^11.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/leaflet": "^1.5.21",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^12.20.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/yup": "^0.29.11",
    "autoprefixer": "^10.2.4",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^5.3.0",
    "customize-cra": "^1.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "nodemon": "^2.0.7",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "typescript": "^4.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
